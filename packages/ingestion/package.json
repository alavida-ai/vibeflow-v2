{
  "name": "@vibeflow/ingestion",
  "version": "2.1.6",
  "description": "",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "build": "tsup src/index.ts --format esm --sourcemap --dts --out-dir dist",
    "dev": "tsup src/index.ts --format esm --sourcemap --dts --out-dir dist --watch",
    "pipeline:mentions": "PIPELINE_TYPE=mentions tsx scripts/run-pipeline.ts",
    "pipeline:tweets": "PIPELINE_TYPE=tweets tsx scripts/run-pipeline.ts",
    "pipeline:replies": "PIPELINE_TYPE=replies tsx scripts/run-pipeline.ts",
    "pipeline:test": "PIPELINE_TYPE=mentions tsx scripts/run-pipeline.ts --user=elonmusk --maxPages=1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@vibeflow/core": "workspace:*",
    "@vibeflow/database": "workspace:*",
    "@vibeflow/logging": "workspace:*",
    "drizzle-orm": "^0.44.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "msw": "^2.8.2",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
