{
  "name": "@vibeflow/media-utils",
  "version": "1.0.0",
  "type": "module",
  "dependencies": {
    "@google/genai": "^1.16.0",
    "uuid": "^11.1.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --sourcemap --dts --out-dir dist",
    "dev": "tsup src/index.ts --format esm --sourcemap --dts --out-dir dist --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.1.6"
  }
}
