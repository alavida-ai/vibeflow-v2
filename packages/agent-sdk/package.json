{
  "name": "@vibeflow/agent-sdk",
  "version": "1.1.3",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --sourcemap --dts --out-dir dist",
    "dev": "tsup src/index.ts --format esm --sourcemap --dts --out-dir dist --watch"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.4",
    "@mastra/client-js": "^0.13.2",
    "@vibeflow/core": "workspace:*",
    "@vibeflow/logging": "workspace:*",
    "drizzle-orm": "^0.44.5",
    "openapi-types": "^12.1.3",
    "yaml": "^2.3.2",
    "zod": "^4.1.11"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^20.5.0",
    "tsup": "^8.5.0",
    "typescript": "^5.1.6"
  }
}
