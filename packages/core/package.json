{
  "name": "@vibeflow/core",
  "version": "1.1.3",
  "type": "module",
  "dependencies": {
    "@vibeflow/database": "workspace:*",
    "@vibeflow/media-utils": "workspace:*",
    "@vibeflow/logging": "workspace:*",
    "drizzle-orm": "^0.44.5",
    "yaml": "^2.3.2"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --sourcemap --dts --out-dir dist",
    "dev": "tsup src/index.ts --format esm --sourcemap --dts --out-dir dist --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./services/media": {
      "import": "./dist/services/media/index.js",
      "default": "./dist/services/media/index.js"
    },
    "./services/database": {
      "import": "./dist/services/database/index.js",
      "default": "./dist/services/database/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "dotenv": "^16.3.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.1.6"
  }
}
