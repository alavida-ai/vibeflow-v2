    "@mastra/libsql": "latest",
    "@mastra/loggers": "latest",
    "@mastra/mcp": "^0.10.12",
    "@mastra/memory": "^0.13.1",
    "@mastra/pg": "^0.14.2",
    "@mastra/rag": "^1.1.0",
    "@octokit/rest": "^21.0.2",
    "@openrouter/ai-sdk-provider": "^0.7.5",
    "@tavily/core": "^0.5.10",
    "ai": "^4.3.19",
    "dotenv": "^17.0.1",
    "hono": "^4.9.5",
    "replicate": "^1.0.1",
    "tsup": "^8.5.0",
